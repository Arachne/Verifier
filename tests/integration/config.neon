nette:
	application:
		mapping:
			*: Tests\Integration\*Presenter
		catchExceptions: true

services:
	routerFactory: Tests\Integration\RouterFactory
	router: @routerFactory::create()
	enabledRuleHandler:
		class: Tests\Integration\EnabledRuleHandler
		tags:
			arachne.verifier.ruleHandler:
				- Tests\Integration\Enabled

extensions:
	kdyby.rules: Kdyby\Annotations\DI\AnnotationsExtension
	arachne.verifier: Arachne\Verifier\DI\VerifierExtension
